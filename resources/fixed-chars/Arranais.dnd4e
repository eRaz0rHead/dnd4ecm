         <loot showpowercard="1" equip-count="0" count="1">
            <RulesElement legality="rules-legal" charelem="330" url="http://www.wizards.com/dndinsider/compendium/ritual.aspx?id=37" internal-id="ID_FMP_RITUAL_37" type="Ritual" name="Water Breathing">
