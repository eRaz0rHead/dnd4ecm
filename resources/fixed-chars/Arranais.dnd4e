         <RulesElement legality="rules-legal" charelem="38" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=359" internal-id="ID_FMP_HYBRID_CLASS_359" type="Hybrid Class" name="Hybrid Warlord">
